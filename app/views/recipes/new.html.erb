<%= form_for(@recipe) do |f|%>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :cook_time %>
    <%= f.text_field :cook_time%>
    <%= f.label :image_url %>
    <%= f.text_field :image%>
    <%= f.label :url%>
    <%= f.text_field :url%>
    <% @tags.each do |tag| %>
        <%= f.check_box :tags, { multiple: true }, tag.id %>
        <%= f.label tag.name%>
    <% end %>
    <%= f.submit %>
<% end %>